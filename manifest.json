{
  "manifest_version": 3,
  "name": "Azure DevOps Board Item Opener",
  "version": "1.0",
  "description": "Opens a new tab with a URL based on a number input",
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "src/popup.html"
  },
  "keyboard": {
    "open_popup": {
      "default": ["Ctrl+3"]
    }
  }
}
